<?xml version="1.0" encoding="utf-8"?>
<GridLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:rowCount="5"
    android:columnCount="5"
    >

    <ImageView
        android:id="@+id/zeroZero"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="100dp" />

    <ImageView
        android:layout_marginTop="100dp"
        android:layout_row="0"
        android:layout_column="1"
        android:src="@drawable/vertical_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_marginTop="100dp"
        android:layout_row="0"
        android:layout_column="2"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:id="@+id/zeroOne"

        />
    <ImageView
        android:layout_marginTop="100dp"
        android:layout_row="0"
        android:layout_column="3"
        android:src="@drawable/vertical_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"

        />
    <ImageView
        android:layout_marginTop="100dp"
        android:layout_row="0"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:layout_column="4"
        android:id="@+id/zeroTwo"
        />

    <ImageView
        android:layout_row="1"
        android:layout_column="0"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:layout_marginLeft="25dp"
        android:clickable="false"

        />

    <ImageView
        android:layout_row="1"
        android:layout_column="1"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"

        />

    <ImageView
        android:layout_row="1"
        android:layout_column="2"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"

        />

    <ImageView
        android:layout_row="1"
        android:layout_column="3"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="1"
        android:layout_column="4"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="2"
        android:layout_column="0"
        android:layout_marginLeft="25dp"
        android:id="@+id/oneZero"
        android:layout_width="71dp"
        android:layout_height="73dp"
        />

    <ImageView
        android:layout_row="2"
        android:layout_column="1"
        android:src="@drawable/vertical_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="2"
        android:layout_column="2"
        android:id="@+id/oneOne"
        android:layout_width="71dp"
        android:layout_height="73dp"
        />
    <ImageView
        android:layout_row="2"
        android:layout_column="3"
        android:src="@drawable/vertical_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />
    <ImageView
        android:layout_row="2"
        android:layout_column="4"
        android:id="@+id/oneTwo"
        android:layout_width="71dp"
        android:layout_height="73dp"
        />

    <ImageView
        android:layout_row="3"
        android:layout_column="0"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        android:layout_marginLeft="25dp"
        />

    <ImageView
        android:layout_row="3"
        android:layout_column="1"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="3"
        android:layout_column="2"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="3"
        android:layout_column="3"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="3"
        android:layout_column="4"
        android:src="@drawable/horizontal_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="4"
        android:layout_column="0"
        android:id="@+id/twoZero"
        android:layout_marginLeft="25dp"
        android:layout_width="71dp"
        android:layout_height="73dp"
        />

    <ImageView
        android:layout_row="4"
        android:layout_column="1"
        android:src="@drawable/vertical_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />

    <ImageView
        android:layout_row="4"
        android:layout_column="2"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:id="@+id/twoOne"

        />
    <ImageView
        android:layout_row="4"
        android:layout_column="3"
        android:src="@drawable/vertical_line_png_images"
        android:layout_width="71dp"
        android:layout_height="73dp"
        android:clickable="false"
        />
    <ImageView
        android:layout_row="4"
        android:layout_column="4"
        android:id="@+id/twotwo"
        android:layout_width="71dp"
        android:layout_height="73dp"
        />


</GridLayout>
